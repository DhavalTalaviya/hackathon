    {
      "type": "ComposedChart",
      "title": "Monthly Costs vs Bookings Over Time",
      "sql": "SELECT m.month, COALESCE(c.total_cost, 0) AS total_cost, COALESCE(b.total_bookings, 0) AS total_bookings FROM (SELECT DISTINCT strftime('%Y-%m', date) AS month FROM costs UNION SELECT DISTINCT strftime('%Y-%m', date) AS month FROM bookings) m LEFT JOIN (SELECT strftime('%Y-%m', date) AS month, SUM(amount) AS total_cost FROM costs GROUP BY strftime('%Y-%m', date)) c ON m.month = c.month LEFT JOIN (SELECT strftime('%Y-%m', date) AS month, COUNT(*) AS total_bookings FROM bookings GROUP BY strftime('%Y-%m', date)) b ON m.month = b.month ORDER BY m.month",
      "xAxisKey": "month",
      "series": [
        {
          "dataKey": "total_cost",
          "name": "Total Costs ($)",
          "type": "bar"
        },
        {
          "dataKey": "total_bookings",
