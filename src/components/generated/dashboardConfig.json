    {
      "type": "BarChart",
      "title": "Cost per Booking by Month",
      "sql": "SELECT m.month, CASE WHEN COALESCE(b.total_bookings, 0) = 0 THEN 0 ELSE ROUND(COALESCE(c.total_cost, 0) * 1.0 / b.total_bookings, 2) END AS cost_per_booking FROM (SELECT DISTINCT strftime('%Y-%m', date) AS month FROM costs UNION SELECT DISTINCT strftime('%Y-%m', date) AS month FROM bookings) m LEFT JOIN (SELECT strftime('%Y-%m', date) AS month, SUM(amount) AS total_cost FROM costs GROUP BY strftime('%Y-%m', date)) c ON m.month = c.month LEFT JOIN (SELECT strftime('%Y-%m', date) AS month, COUNT(*) AS total_bookings FROM bookings GROUP BY strftime('%Y-%m', date)) b ON m.month = b.month WHERE COALESCE(b.total_bookings, 0) > 0 ORDER BY m.month",
